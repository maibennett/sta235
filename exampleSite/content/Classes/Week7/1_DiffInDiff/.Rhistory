dd_labels <- tribble(
~Group, ~Time, ~label,
"Treatment", "Before", "A",
"Treatment", "After", "B",
"Control", "Before", "C",
"Control", "After", "D"
)
dd_data <- tribble(
~Group, ~Before, ~Intervention, ~After,
"Treatment", 30, 35, 50,
"Control",   20, 25, 30
) %>%
pivot_longer(cols = !Group, names_to = "Time", values_to = "avg") %>%
left_join(dd_labels, by = c("Group", "Time")) %>%
mutate(across(c(Group, Time), ~fct_inorder(.)))
ggplot(dd_data, aes(x = Time, y = avg, color = Group, group = Group)) +
geom_vline(xintercept = "Intervention") +
geom_line(size = 3,
key_glyph = draw_key_point) +
ggplot2::annotate(geom = "segment", x = "Intervention", xend = "After",
y = 35, yend = 40, color = "#900DA4", size = 2, linetype = "22") +
ggplot2::annotate(geom = "segment", x = "After", xend = "After",
y = 40, yend = 50, color = "#FF4136", size = 2) +
ggplot2::annotate(geom = "label", x = "After", y = 44, label = "Causal\neffect",
family = "Fira Sans Condensed", fontface = "bold",
size = 4, fill = "#FF4136", color = "white") +
geom_point(data = filter(dd_data, !is.na(label)), size = 15) +
geom_text(aes(label = label), family = "Fira Sans Condensed",
fontface = "bold", size = 9, color = "white") +
scale_color_manual(values = c("#900DA4", "#F89441")) +
labs(x = NULL, y = "Outcome") +
coord_cartesian(ylim = c(15, 55)) +
theme_ipsum_fsc(plot_title_face = "bold",plot_title_size = 24) +
theme(axis.title.x = element_text(size=18),#margin = margin(t = 10, r = 0, b = 0, l = 0)),
axis.text.x = element_text(size=18),
axis.title.y = element_text(size=18),#margin = margin(t = 0, r = 10, b = 0, l = 0)),
axis.text.y = element_blank(),
legend.title = element_blank(),
legend.text = element_text(size=15),
legend.background = element_rect(fill="white",colour ="white"),
title = element_text(size=14))
